#!/bin/sh
set -eux

brew bundle --file=- <<EOF
cask "1password"
cask "brave-browser"
brew "coreutils"
cask "docker"
brew "go"
brew "graphviz"
brew "htop"
brew "imagemagick@6"
cask "iterm2"
brew "md5sha1sum"
cask "ngrok"
cask "roboform"
cask "rubymine"
cask "slack"
cask "visual-studio-code"
brew "watch"
EOF

brew link --force --overwrite imagemagick@6

ln -sf /bin/mkdir /usr/local/bin/gmkdir
ln -sf /usr/bin/install /usr/local/bin/ginstall
